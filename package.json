{
  "name": "iobroker.gigaset-elements",
  "version": "0.1.0",
  "description": "Adapter for Gigaset Elements",
  "author": {
    "name": "matthsc",
    "email": "matthsc@gmx.net"
  },
  "homepage": "https://github.com/matthsc/ioBroker.gigaset-elements",
  "license": "MIT",
  "keywords": [
    "gigaset elements",
    "security",
    "alarm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/ioBroker.gigaset-elements"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.1",
    "gigaset-elements-api": "^0.2.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.5.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.5.1",
    "@alcalzone/release-script-plugin-license": "^3.5.0",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@iobroker/adapter-react": "^2.0.17",
    "@iobroker/testing": "^2.5.2",
    "@material-ui/core": "^4.12.3",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.9",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sinon": "^10.0.9",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "axios": "^0.25.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "date-fns": "^2.28.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "gulp": "^4.0.2",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.5.1",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build --hmr-port 1235",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha --require ts-node/register test/integration.ts --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "check-all": "npm run check && npm run lint && npm run test",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "release": "release-script -p iobroker license"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/matthsc/ioBroker.gigaset-elements/issues"
  },
  "readmeFilename": "README.md"
}
