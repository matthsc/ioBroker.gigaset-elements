{
  "name": "iobroker.gigaset-elements",
  "version": "0.4.0",
  "description": "Adapter for Gigaset Elements",
  "author": {
    "name": "matthsc",
    "email": "matthsc@gmx.net"
  },
  "homepage": "https://github.com/matthsc/ioBroker.gigaset-elements",
  "license": "MIT",
  "keywords": [
    "gigaset elements",
    "security",
    "alarm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matthsc/ioBroker.gigaset-elements"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.8",
    "gigaset-elements-api": "~0.5.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.6.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.6.0",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/adapter-react-v5": "^4.6.6",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.14.13",
    "@mui/material": "^5.14.13",
    "@mui/styles": "^5.14.13",
    "@tsconfig/node18": "^18.2.2",
    "@types/chai": "^4.3.8",
    "@types/chai-as-promised": "^7.1.7",
    "@types/mocha": "^10.0.2",
    "@types/node": "^18.18.5",
    "@types/proxyquire": "^1.3.29",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "@types/sinon": "^10.0.19",
    "@types/sinon-chai": "^3.2.10",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.5",
    "sinon": "^16.1.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "translate": "translate-adapter",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha --require ts-node/register test/integration.ts --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "check-all": "npm run check && npm run lint && npm run test",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "prettier": "prettier --config .prettierrc.js \"{src,test}/**/*.ts\" --write",
    "release": "release-script -p iobroker license"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/matthsc/ioBroker.gigaset-elements/issues"
  },
  "readmeFilename": "README.md"
}
